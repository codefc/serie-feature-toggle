<!-- Search Form -->

<section class="mbr-section form3 cid-rTcVga2lJ0" id="form3-3">
    <div class="container">
        <div class="row justify-content-center">
            <div class="title col-12 col-lg-8">
                <h2 class="align-center pb-2 mbr-fonts-style display-2">Put some dog breed</h2>
                <h3 class="mbr-section-subtitle align-center pb-5 mbr-light mbr-fonts-style display-5">
                    and click on search button</h3>
            </div>
        </div>

        <div class="row py-2 justify-content-center">
            <div class="col-12 col-lg-6  col-md-8 " data-form-type="formoid">
                <!---Formbuilder Form--->
                <form  method="POST" class="mbr-form form-with-styler" data-form-title="Dog Breed Form">
                    <div class="dragArea row">
                        <div class="form-group col" data-for="breed">
                            <input type="text" name="breed" placeholder="Breed" data-form-field="Brees" required="required" class="form-control display-7" id="email-form3-3">
                        </div>
                        <div class="col-auto input-group-btn"><button type="submit" class="btn btn-primary display-4">SEARCH</button></div>
                    </div>
                </form><!---Formbuilder Form--->
            </div>
        </div>
    </div>
</section>
